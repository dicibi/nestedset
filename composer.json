{
    "name": "kalnoy/nestedset",
    "description": "Nested Set Model for Laravel 5.7 and up",
    "keywords": [
        "laravel",
        "nested sets",
        "nsm",
        "database",
        "hierarchy"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Kalnoy",
            "email": "lazychaser@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/events": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^2.0",
        "larastan/larastan": "v2.9.2",
        "laravel/pint": "^1.14"
    },
    "autoload": {
        "psr-4": {
            "Kalnoy\\Nestedset\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "v5.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Kalnoy\\Nestedset\\NestedSetServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": [
            "@php ./vendor/bin/phpunit"
        ]
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
